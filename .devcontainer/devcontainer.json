{
	"name": "Epita Container",
	"image": "ghcr.io/francoisdtm/epita-container:git-main-latest",
	"containerUser": "epita",
	"settings": {
		"terminal.integrated.shell.linux": "/bin/zsh"
	},
	"mounts": [
		"source=vscode-epita-data,target=/home/epita/.vscode-server,type=volume"
	],
	"extensions": [
		"ms-vscode.cpptools",
		"ms-python.python",
		"eamodio.gitlens",
		"znck.grammarly",
		"formulahendry.code-runner",
		"ms-vsliveshare.vsliveshare",
		"tabnine.tabnine-vscode"
	]
}